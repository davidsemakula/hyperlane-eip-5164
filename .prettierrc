{
  "printWidth": 120,
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "tabWidth": 4,
        "useTabs": false,
        "singleQuote": false,
        "bracketSpacing": false
      }
    },
    {
      "files": "*.ts",
      "options": {
        "tabWidth": 2,
        "useTabs": false,
        "trailingComma": "all",
        "singleQuote": true,
        "semi": true
      }
    }
  ]
}